<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>ImgFlipApi</id>
        <version>1.0.1</version>
        <authors>github@thibautvial.com</authors>
